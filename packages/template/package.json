{
  "name": "@zana/template",
  "version": "0.1.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "echo 'Tests executed at monorepo root'"
  },
  "dependencies": {
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}
