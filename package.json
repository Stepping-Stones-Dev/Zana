    "googleapis": "^152.0.0",
    "js-cookie": "^3.0.5",
    "micro": "^10.0.1",
    "passport": "^0.7.0",
    "passport-saml": "^3.2.4",
  "react": "18.3.1",
  "react-dom": "18.3.1",
    "tailwind-variants": "0.1.20",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
