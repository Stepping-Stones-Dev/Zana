{
  "name": "zana",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "pnpm run ngrok && set TURBO_LOGS=logs && turbo run dev",
    "build": "pnpm run ngrok && set TURBO_LOGS=logs && turbo run build",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test",
    "clean": "rimraf --glob \"./dist\" \"./build\" \"./out\" \"./.next\" \"./apps/*/.next\" \"./packages/*/dist\" \"./packages/*/build\" \"./packages/*/.next\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "changeset publish",
    "deploy:all": "pnpm run ngrok && pnpm run build && echo 'Deploy step here'",
    "ngrok": "start \"ngrok\" cmd /c \"ngrok http --domain=workable-warthog-highly.ngrok-free.app 3000\""
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@types/js-cookie": "^3.0.6",
    "@types/micro": "^7.3.7",
    "@types/node": "20.5.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "autoprefixer": "10.4.19",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "14.0.0",
    "next": "15.3.1",
    "postcss": "8.4.49",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.6"
  }
}
