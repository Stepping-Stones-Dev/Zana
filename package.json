{
  "name": "sam",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "turbo run dev --filter=@sam/web",
    "dev:tunnel": "start \"ngrok\" cmd /c \"ngrok http --domain=workable-warthog-highly.ngrok-free.app 3000\" && pnpm --filter @sam/web run dev",
    "build": "turbo run build",
    "start": "turbo run start --filter=@sam/web",
    "dev:mono": "turbo run dev --filter=@sam/web",
    "build:mono": "turbo run build",
    "start:mono": "turbo run start --filter=@sam/web",
    "start:tunnel": "start \"ngrok\" cmd /c \"ngrok http --domain=workable-warthog-highly.ngrok-free.app 3000\" && next start -p 3000",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "i18n:check": "node apps/web/scripts/check-i18n.mjs"
  },
  "dependencies": {
    "@module-federation/nextjs-mf": "^7.0.0",
