{
  "name": "zana",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.18.2",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest --coverage --coverageReporters=text --coverageReporters=text-summary --passWithNoTests",
    "sbom": "node scripts/generate-sbom.cjs",
    "check:licenses": "node scripts/check-licenses.cjs",
    "security:scan": "node scripts/security-scan.cjs --fail-on high",
    "clean": "rimraf --glob \"./dist\" \"./build\" \"./out\" \"./packages/*/dist\" \"./packages/*/build\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "changeset publish",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:changed": "node scripts/run-changed.js lint",
    "test:changed": "node scripts/run-changed.js test",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "storybook": "pnpm --filter @zana/ui run storybook"
  },
  "dependencies": {
    "@react-aria/visually-hidden": "3.8.28",
    "@react-types/shared": "3.32.1",
    "@tailwindcss/postcss": "4.1.11",
    "@tailwindcss/vite": "4.1.11",
    "clsx": "2.1.1",
    "framer-motion": "11.18.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.23.0",
    "tailwind-variants": "3.1.1",
    "tailwindcss": "4.1.11"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@cyclonedx/cyclonedx-npm": "^4.0.3",
    "@eslint/compat": "1.2.8",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.25.1",
    "@jest/types": "^30.2.0",
    "@types/node": "20.5.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.46.0",
    "@vitejs/plugin-react": "4.7.0",
    "eslint": "^9.35.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "16.4.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-css-modules-transform": "^4.4.2",
    "license-checker": "^25.0.1",
    "postcss": "8.5.6",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.6",
    "typescript": "5.6.3",
    "vite": "6.0.11",
    "vite-tsconfig-paths": "5.1.4"
  }
}
