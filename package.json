{
  "name": "zana",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "dev": "pnpm run ngrok && set TURBO_LOGS=logs && turbo run dev",
    "build": "turbo run build",
    "format": "turbo run format",
    "pretest": "pnpm --filter @zana/telemetry run lint:fix",
    "test": "jest --coverage --coverageReporters=text --coverageReporters=text-summary --passWithNoTests",
    "sbom": "node scripts/generate-sbom.cjs",
    "check:licenses": "node scripts/check-licenses.cjs",
    "security:scan": "node scripts/security-scan.cjs --fail-on high",
    "clean": "rimraf --glob \"./dist\" \"./build\" \"./out\" \"./.next\" \"./apps/*/.next\" \"./packages/*/dist\" \"./packages/*/build\" \"./packages/*/.next\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "changeset publish",
    "deploy:all": "pnpm run ngrok && pnpm run build && echo 'Deploy step here'",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:changed": "node scripts/run-changed.js lint",
    "test:changed": "node scripts/run-changed.js test",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "ngrok": "start \"ngrok\" cmd /c \"ngrok http --domain=workable-warthog-highly.ngrok-free.app 3000\""
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.5.4",
    "@changesets/cli": "^2.29.7",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "9.25.1",
    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.6",
    "@types/micro": "^10.0.0",
    "@types/node": "20.5.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "19.0.1",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "autoprefixer": "10.4.19",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "eslint-import-resolver-typescript": "3.6.3",
    "globals": "16.4.0",
    "next": "15.4.7",
    "postcss": "8.4.49",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.6",
    "typescript": "5.6.3",
    "@cyclonedx/cyclonedx-npm": "^4.0.3",
    "license-checker": "^25.0.1",
    "husky": "^9.1.6"
  }
}
